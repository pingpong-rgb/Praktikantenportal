<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="intern_intern_portal_rule" model="ir.rule">
        <field name="name">Intern: Portal sieht nur eigene Daten</field>
        <field name="model_id" ref="model_intern_intern"/>
        <field name="groups" eval="[(4, ref('praktikanten_modul.group_intern_portal'))]"/>
        <field name="domain_force">[('partner_id', '=', user.partner_id.id)]</field>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="False"/>
        <field name="perm_create" eval="False"/>
        <field name="perm_unlink" eval="False"/>
    </record>
</odoo>
<odoo>
    <record id="view_intern_onboarding_list" model="ir.ui.view">
        <field name="name">intern.onboarding.list</field>
        <field name="model">intern.onboarding</field>
        <field name="arch" type="xml">
            <list>
                <field name="sequence"/>
                <field name="name"/>
                <field name="file_name"/>
                <field name="link_url"/>
                <field name="active"/>
            </list>
        </field>
    </record>

    <record id="view_intern_onboarding_form" model="ir.ui.view">
        <field name="name">intern.onboarding.form</field>
        <field name="model">intern.onboarding</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="description"/>
                        <field name="file" filename="file_name"/>
                        <field name="link_url"/>
                        <field name="active"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_intern_onboarding" model="ir.actions.act_window">
        <field name="name">Onboarding</field>
        <field name="res_model">intern.onboarding</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="menu_intern_info_root"
          name="Informationen"
          parent="praktikanten_modul.menu_intern_root"
          sequence="50"/>

    <menuitem id="menu_intern_onboarding"
          name="Onboarding"
          parent="praktikanten_modul.menu_intern_info_root"
          action="action_intern_onboarding"
          sequence="10"/>
</odoo>